{"ast":null,"code":"import parserMixin from '@/mixins/mixin';\nimport dataService from '../services/dataService';\nimport VueDatePicker from '@vuepic/vue-datepicker';\nimport moment from 'moment';\nimport coordsMixin from '@/mixins/coordsMixin';\nimport { YandexMap, YandexMarker } from \"vue-yandex-maps\";\nimport { loadYmap } from \"vue-yandex-maps\";\nexport default {\n  components: {\n    datepicker: VueDatePicker,\n    YandexMap,\n    YandexMarker\n  },\n  mixins: [coordsMixin],\n  data() {\n    return {\n      //Polja u Klasi Let\n\n      // private String id;\n\n      // private Adresa lokOd;\n      // private Adresa lokDo;\n\n      // private String brojLeta;\n      // private int kapacitet;\n      // private double cena;\n      // private LocalDateTime datumIVreme;\n      // @DBRef\n      // @Nullable\n      // private Collection<RegKor> listaPutnika;\n\n      flight: {\n        lokOd: new Object(),\n        lokDo: new Object(),\n        //ovo bi trebalo generisati automatski al ajd\n        brojLeta: -1,\n        kapacitet: -1,\n        cena: -1,\n        datumIVreme: new Date()\n      },\n      lokacija: new Object()\n    };\n  },\n  mounted() {},\n  created() {},\n  methods: {\n    updateDatePicker(val1) {\n      console.log(\"VAL1: \" + val1);\n      let tempDatum = moment(val1).format('YYYY-MM-DD');\n      console.log(\"TEMP DATUM: \" + tempDatum);\n      this.flight.datumIVreme = tempDatum;\n    },\n    createNewFlight() {\n      console.log(\"ODREDJIVANJE KOORDINATA\");\n      coordsMixin.methods.getCoordsFromAdresa(this.flight.lokOd);\n    },\n    async getCoordsFromAdresa(adresa) {\n      return ymaps.geocode(adresa).then(result => {\n        let locationCoords = {\n          coords: result.geoObjects.get(0).geometry.getCoordinates()\n        };\n        this.lokacija.lat = locationCoords.coords[0];\n        this.lokacija.lng = locationCoords.coords[1];\n        this.form.adresa.lat = this.lokacija.lat;\n        this.form.adresa.lng = this.lokacija.lng;\n        console.log(\"LOKACIJA: \" + JSON.stringify(this.lokacija));\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAyDA,OAAOA,WAAU,MAAO,gBAAe;AACvC,OAAOC,WAAU,MAAO,yBAAwB;AAChD,OAAOC,aAAY,MAAO,wBAAwB;AAClD,OAAOC,MAAK,MAAO,QAAO;AAE1B,OAAOC,WAAU,MAAO,sBAAqB;AAC7C,SAAQC,SAAS,EAAEC,YAAY,QAAO,iBAAgB;AACtD,SAASC,QAAO,QAAS,iBAAiB;AAE1C,eAAe;EACXC,UAAU,EAAC;IACPC,UAAU,EAACP,aAAa;IACxBG,SAAS;IAAEC;EACf,CAAC;EACDI,MAAM,EAAE,CAACN,WAAW,CAAC;EACrBO,IAAI,GAAE;IACF,OAAM;MACF;;MAEA;;MAEA;MACA;;MAEA;MACA;MACA;MACA;MACA;MACA;MACA;;MAGAC,MAAM,EAAC;QACHC,KAAK,EAAE,IAAIC,MAAM,EAAE;QACnBC,KAAK,EAAE,IAAID,MAAM,EAAE;QACnB;QACAE,QAAQ,EAAE,CAAC,CAAC;QACZC,SAAS,EAAE,CAAC,CAAC;QACbC,IAAI,EAAE,CAAC,CAAC;QACRC,WAAW,EAAE,IAAIC,IAAI;MACzB,CAAC;MACDC,QAAQ,EAAE,IAAIP,MAAM;IACxB;EACJ,CAAC;EACDQ,OAAO,GAAE,CAET,CAAC;EACDC,OAAO,GAAE,CAET,CAAC;EACDC,OAAO,EAAC;IACJC,gBAAgB,CAACC,IAAI,EAAC;MAClBC,OAAO,CAACC,GAAG,CAAC,QAAO,GAAKF,IAAI,CAAC;MAC7B,IAAIG,SAAQ,GAAI1B,MAAM,CAACuB,IAAI,CAAC,CAACI,MAAM,CAAC,YAAY,CAAC;MACjDH,OAAO,CAACC,GAAG,CAAC,cAAa,GAAKC,SAAS,CAAC;MACxC,IAAI,CAACjB,MAAM,CAACO,WAAU,GAAIU,SAAS;IACvC,CAAC;IACDE,eAAe,GAAE;MACbJ,OAAO,CAACC,GAAG,CAAC,yBAAyB,CAAC;MACtCxB,WAAW,CAACoB,OAAO,CAACQ,mBAAmB,CAAC,IAAI,CAACpB,MAAM,CAACC,KAAK,CAAC;IAC9D,CAAC;IACD,MAAMmB,mBAAmB,CAACC,MAAM,EAAC;MAC7B,OAAOC,KAAK,CAACC,OAAO,CAACF,MAAM,CAAC,CAACG,IAAI,CAAEC,MAAM,IAAK;QAC1C,IAAIC,cAAa,GAAI;UACjBC,MAAM,EAAEF,MAAM,CAACG,UAAU,CAACC,GAAG,CAAC,CAAC,CAAC,CAACC,QAAQ,CAACC,cAAc;QAC5D,CAAC;QACD,IAAI,CAACtB,QAAQ,CAACuB,GAAE,GAAIN,cAAc,CAACC,MAAM,CAAC,CAAC,CAAC;QAC5C,IAAI,CAAClB,QAAQ,CAACwB,GAAE,GAAIP,cAAc,CAACC,MAAM,CAAC,CAAC,CAAC;QAC5C,IAAI,CAACO,IAAI,CAACb,MAAM,CAACW,GAAE,GAAI,IAAI,CAACvB,QAAQ,CAACuB,GAAG;QACxC,IAAI,CAACE,IAAI,CAACb,MAAM,CAACY,GAAE,GAAI,IAAI,CAACxB,QAAQ,CAACwB,GAAG;QACxClB,OAAO,CAACC,GAAG,CAAC,YAAW,GAAImB,IAAI,CAACC,SAAS,CAAC,IAAI,CAAC3B,QAAQ,CAAC,CAAC;MAC7D,CAAC,CAAC;IACN;EACJ;AAEJ","names":["parserMixin","dataService","VueDatePicker","moment","coordsMixin","YandexMap","YandexMarker","load